#include<bits/stdc++.h>
using namespace std;
void find_span(int arr[],int n)
{
    int span[n];
    span[0] = 1;
    stack<int> s;
    s.push(0);
    for(int i=1;i<n;i++)
    {
        
        while(!s.empty() && arr[s.top()] <= arr[i])
        {
            s.pop();
        }
        if(!s.empty())
            span[i] = i - s.top();
        else
            span[i] = i+1;
        
        s.push(i);
    }
    for(int i=0;i<n;i++)
        cout<<span[i]<<" ";
    cout<<endl;
}
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int arr[n];
        int i;
        for(i=0;i<n;i++)
            cin>>arr[i];
            
        find_span(arr,n);
    }
	return 0;
}
